-- Steal a Brainrot: Secret Hunter + Contact Lock
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "SecretAccess"
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false

-- 1. THE WHITE POP-UP (MANDATORY CONTACT)
local LockFrame = Instance.new("Frame")
LockFrame.Parent = ScreenGui
LockFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
LockFrame.Size = UDim2.new(0, 400, 0, 180)
LockFrame.Position = UDim2.new(0.5, -200, 0.5, -90)
LockFrame.BorderSizePixel = 4

local Msg = Instance.new("TextLabel")
Msg.Parent = LockFrame
Msg.Text = "text 786-366-7302 for the accual script thank you"
Msg.Size = UDim2.new(1, 0, 0.6, 0)
Msg.TextColor3 = Color3.fromRGB(0, 0, 0)
Msg.TextSize = 20
Msg.Font = Enum.Font.SourceSansBold
Msg.TextWrapped = true

local OpenBtn = Instance.new("TextButton")
OpenBtn.Parent = LockFrame
OpenBtn.Text = "I TEXTED - OPEN HUNTER"
OpenBtn.Size = UDim2.new(0.8, 0, 0.25, 0)
OpenBtn.Position = UDim2.new(0.1, 0, 0.7, 0)
OpenBtn.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
OpenBtn.TextColor3 = Color3.fromRGB(255, 255, 255)

-- 2. THE SECRET HUNTER MENU
local MainMenu = Instance.new("Frame")
MainMenu.Visible = false
MainMenu.Parent = ScreenGui
MainMenu.BackgroundColor3 = Color3.fromRGB(20, 0, 40)
MainMenu.Size = UDim2.new(0, 250, 0, 200)
MainMenu.Position = UDim2.new(0.5, -125, 0.5, -100)
MainMenu.Active = true
MainMenu.Draggable = true

local Title = Instance.new("TextLabel")
Title.Parent = MainMenu
Title.Text = "SECRET FINDER"
Title.Size = UDim2.new(1, 0, 0, 40)
Title.TextColor3 = Color3.fromRGB(0, 255, 255)

local EspBtn = Instance.new("TextButton")
EspBtn.Parent = MainMenu
EspBtn.Text = "HIGHLIGHT SECRETS/OGS"
EspBtn.Size = UDim2.new(0.8, 0, 0, 40)
EspBtn.Position = UDim2.new(0.1, 0, 0.3, 0)
EspBtn.BackgroundColor3 = Color3.fromRGB(70, 0, 150)

local TpBtn = Instance.new("TextButton")
TpBtn.Parent = MainMenu
TpBtn.Text = "TP TO RAREST ITEM"
TpBtn.Size = UDim2.new(0.8, 0, 0, 40)
TpBtn.Position = UDim2.new(0.1, 0, 0.6, 0)
TpBtn.BackgroundColor3 = Color3.fromRGB(0, 150, 0)

-- LOGIC
OpenBtn.MouseButton1Click:Connect(function()
    LockFrame.Visible = false
    MainMenu.Visible = true
end)

EspBtn.MouseButton1Click:Connect(function()
    for _, v in pairs(workspace:GetDescendants()) do
        -- Scans for Brainrots with high value/rarity names
        if v:IsA("Model") and (v.Name:find("OG") or v.Name:find("Secret") or v.Name:find("Mythic")) then
            local Box = Instance.new("SelectionBox")
            Box.Adornee = v
            Box.Parent = v
            Box.Color3 = Color3.fromRGB(0, 255, 255)
        end
    end
end)

TpBtn.MouseButton1Click:Connect(function()
    for _, v in pairs(workspace:GetDescendants()) do
        if v.Name:find("Secret") or v.Name:find("OG") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:GetModelCFrame()
            break
        end
    end
end)
